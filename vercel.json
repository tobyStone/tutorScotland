{
  "version": 2,
  "rewrites": [
    {
      "source": "/",
      "destination": "/public/index.html"
    },
    {
      "source": "/parents",
      "destination": "/public/parents.html"
    },
    {
      "source": "/tutorszone",
      "destination": "/public/tutorszone.html"
    },
    {
      "source": "/admin",
      "destination": "/public/admin.html"
    },
    {
      "source": "/login",
      "destination": "/public/login.html"
    },
    {
      "source": "/tutors/:slug*",
      "destination": "/api/tutors.js?slug=:slug"
    },
    {
      "source": "/about",
      "destination": "/about-us.html"
    },
    {
      "source": "/api/login",
      "destination": "/api/login.js"
    },
    {
      "source": "/api/parents",
      "destination": "/api/parents.js"
    },
    {
      "source": "/api/addTutor",
      "destination": "/api/addTutor.js"
    },

    {
      "source": "/api/admin",
      "destination": "/api/admin.js"
    }
  ]
}
